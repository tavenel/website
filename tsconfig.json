{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": [
				"src/assets/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@css/*": [
				"src/css/*"
			],
			"@cours/*": [
				"src/pages/cours/*"
			],
			"@courses/*": [
				"src/courses/*"
			],
			"@icons/*": [
				"src/icons/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@ts/*": [
				"src/ts/*"
			],
		}
	}
}
