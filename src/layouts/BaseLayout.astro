---
import Head from '@components/Head.astro';
import Header from '@components/Header.astro';
import Footer from '@components/Footer.astro';

const { title, url } = Astro.props;
---

<!doctype html>
<html lang="en">
	<Head title={title} url={url}/>
	<body class="light">
		<Header />
		<main id="main">
			<slot />
		</main>
		<Footer />
	</body>
</html>

